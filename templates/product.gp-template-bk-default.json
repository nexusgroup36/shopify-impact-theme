script>\n  let currentReview = 1;\n  const totalReviews = 20;\n\n  function showReview(index) {\n    for (let i = 1; i <= totalReviews; i++) {\n      const el = document.getElementById('review' + i);\n      if (el) el.style.display = 'none';\n    }\n    const showEl = document.getElementById('review' + index);\n    if (showEl) showEl.style.display = 'flex';\n    currentReview = index;\n  }\n\n  function showNextReview() {\n    const nextReview = currentReview === totalReviews ? 1 : currentReview + 1;\n    showReview(nextReview);\n  }\n\n  function showPreviousReview() {\n    const prevReview = currentReview === 1 ? totalReviews : currentReview - 1;\n    showReview(prevReview);\n  }\n\n  showReview(currentReview);\n</script>\n<script>\n  setInterval(() => {\n    showNextReview();\n  }, 5000); // 7000 ms = 3 seconden, pas aan indien nodig\n</script>"
